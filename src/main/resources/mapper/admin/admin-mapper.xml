<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mogreene.adminmpa.admin.repository.AdminRepository">

    <!-- admin Login -->
    <select id="loginAdmin" resultType="AdminDTO" parameterType="AdminDTO">
        SELECT *

        FROM admin

        WHERE username = #{username}

        AND password = #{password}
    </select>
</mapper>
